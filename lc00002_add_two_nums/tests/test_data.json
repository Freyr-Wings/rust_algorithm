[
  {
    "test_name": "SimpleAdd",
    "l1": [2, 4, 3],
    "l2": [5, 6, 4],
    "expected": [7, 0, 8]
  },
  {
    "test_name": "BothZero",
    "l1": [0],
    "l2": [0],
    "expected": [0]
  },
  {
    "test_name": "SingleCarry",
    "l1": [1, 9],
    "l2": [9],
    "expected": [0, 0, 1]
  },
  {
    "test_name": "MultipleCarries",
    "l1": [9, 9, 9],
    "l2": [1],
    "expected": [0, 0, 0, 1]
  },
  {
    "test_name": "DifferentLengths",
    "l1": [1, 2, 3, 4],
    "l2": [9, 9],
    "expected": [0, 2, 4, 4]
  },
  {
    "test_name": "LargeNumbers",
    "l1": [9, 9, 9, 9, 9, 9, 9],
    "l2": [9, 9, 9, 9],
    "expected": [8, 9, 9, 9, 0, 0, 0, 1]
  }
]
